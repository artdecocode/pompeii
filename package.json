{
  "name": "pompeii",
  "version": "1.0.0",
  "description": "A lambda function to notify of npm releases.",
  "main": "build",
  "scripts": {
    "t": "zoroaster --babel",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn t test/spec",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test --watch",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "build": "babel src --out-dir build --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/pompeii.git"
  },
  "keywords": [
    "pompeii"
  ],
  "maintainers": [
    {
      "name": "Art Deco",
      "email": "artdeco@adc.sh",
      "url": "https://artd.eco"
    }
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/pompeii/issues"
  },
  "homepage": "https://github.com/artdecocode/pompeii#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "3.11.2"
  }
}
